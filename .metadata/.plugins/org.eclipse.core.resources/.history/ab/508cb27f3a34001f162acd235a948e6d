package com.anurag.computer_inventory_system;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.autoconfigure.domain.EntityScan;

@SpringBootApplication
@EntityScan(basePackages = "com.anurag.computer_inventory_system.entities")
public class ComputerInventorySystemApplication {
    public static void main(String[] args) {
        SpringApplication.run(ComputerInventorySystemApplication.class, args);
    }
}
